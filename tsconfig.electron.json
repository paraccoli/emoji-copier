{
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "outDir": ".vite/build",
    "sourceMap": true,
    "noImplicitAny": true
  },
  "include": ["src/main/**/*.ts"],
  "exclude": ["node_modules"]
}